<%= content_for :content do %>
  <%= render 'shared/header' %>
  <div class="container">
    <%= form_for @design, :url => :upload_callback, :html => {:multipart => true} do |f| %>
      <p>
        <%= f.file_field :file %>
        <%= f.hidden_field :file_cache %>
        <%= f.submit "done" %>
      </p>
    <% end %>
  </div>
<% end %>